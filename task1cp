def checkout() {
	dir (BUILD_ID) {
		checkout([$class: 'GitSCM', branches: [[name: '*/moba']], extensions: [], userRemoteConfigs: [[credentialsId: 'c90309e1-04e5-4514-b929-7742883dc019', url: 'https://github.com/bhanunew-org/test.git']]])

}

}
node {
	stage ('checkout git repo'){
		checkout()
}




	stage ('making a file using case') {
	if (env.selection_number == "1"){ 
	sh "sudo touch /root/file1test.txt" }
	else if (env.selection_number == "2") { 
	sh "sudo touch /root/file2test.txt"}
	else {
	sh "sudo touch /root/file3test.txt" }
	}











	stage ('displaying folder path') {
	 sh "echo this is git branch name ${env.GIT_BRANCH}"
	 sh "echo this is build number ${env.BUILD_NUMBER}"
	 sh "echo URL that will redirect to a Job in a preferred user interface ${env.JOB_DISPLAY_URL}"
	 sh "echo Short Name of the project of this build stripping off folder paths ${env.JOB_BASE_NAME}"
	 sh "echo Name of the project of this build ${env.JOB_NAME}"
	 sh "echo this will be set to the name of the branch being built ${env.BRANCH_NAME}"
	 sh "echo this will be set to a timestamp in the format ${env.TAG_DATE}"
	 sh "echo Name of the agent if the build is on an agent ${env.NODE_NAME}"
	 sh "echo build number ${currentBuild.number}"} 
stage('Set branch name') {
                    currentBuild.displayName ="${currentBuild.number} ${currentBuild.displayName}"
                
        }
//
//	stage ('changing the status') {
//	currentBuild.displayName=+env.WORKSPACE_TMP+ env.JENKINS_HOME

// }








